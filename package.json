{
  "name": "1188-web",
  "private": true,
  "author": "Ezeikel Pemberton (https://ezeikel.dev)",
  "version": "1.0.0",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev": "next && yarn test",
    "build": "next build",
    "check-types": "jest --config test/jest.tsc.js",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "start": "next start -p 8000",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "yarn build",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "validate": "yarn test:coverage && yarn test:e2e:run",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:e2e:run"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|ts|tsx|css|html|md)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/pro-light-svg-icons": "^5.6.3",
    "@fortawesome/pro-regular-svg-icons": "^5.6.3",
    "@fortawesome/pro-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@testing-library/cypress": "^5.0.2",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/jest": "^24.0.23",
    "@types/nprogress": "^0.2.0",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^4.4.1",
    "@types/yup": "^0.26.27",
    "axios": "^0.19.0",
    "body-scroll-lock": "^2.6.4",
    "formik": "^1.5.8",
    "next": "^9.1.1",
    "nprogress": "^0.2.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-fontawesome": "^1.6.1",
    "react-slick": "^0.25.2",
    "styled-components": "^4.4.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@fortawesome/fontawesome-pro": "^5.6.3",
    "@storybook/react": "^4.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "cypress": "^3.8.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "jest-runner-tsc": "^1.6.0",
    "jest-styled-components": "^6.3.4",
    "jest-watch-select-projects": "^1.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "typescript": "^3.7.3"
  }
}
