{
  "name": "1188-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "jest --config test/jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "pnpm prettier --write",
    "check-format": "pnpm prettier --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit ./next-env.d.ts ./custom.d.ts"
    ],
    "**/*.+(js|jsx|json|ts|tsx|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests --passWithNoTests"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/pro-light-svg-icons": "^6.5.1",
    "@fortawesome/pro-regular-svg-icons": "^6.5.1",
    "@fortawesome/pro-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@sentry/nextjs": "^7.91.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/jest": "^29.5.11",
    "@types/nprogress": "^0.2.3",
    "@types/react-fontawesome": "^1.6.8",
    "@types/react-slick": "^0.23.13",
    "@types/styled-components": "^5.1.34",
    "@types/yup": "^0.32.0",
    "axios": "^1.6.3",
    "body-scroll-lock": "4.0.0-beta.0",
    "formik": "^2.4.5",
    "next": "14.0.4",
    "nprogress": "^0.2.0",
    "postmark": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-slick": "^0.29.0",
    "styled-components": "^6.1.6",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@fortawesome/fontawesome-pro": "^6.5.1",
    "@storybook/react": "^7.6.6",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/testing-library__cypress": "^5.0.13",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-eslint": "^2.1.2",
    "jest-styled-components": "^7.2.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "start-server-and-test": "^2.0.3",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5.3.3"
  }
}
